{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "settings": {
    "import/extensions": [
      ".mjs",
      ".js",
      ".cjs"
    ],
    "import/resolver": {
      "node": {
        "extensions": [
          ".js"
        ]
      }
    }
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:node/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 22,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "prettier/prettier": "error",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "operator-linebreak": "off",
    "no-console": "off"

    //    "no-unused-vars": "warn",
    //    "no-console": "off",
    //    "func-names": "off",
    //    "no-process-exit": "off",
    //    "object-shorthand": "off",
    //    "class-methods-use-this": "off"
  }
}